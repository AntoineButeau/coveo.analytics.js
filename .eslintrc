// ESLint documentation can be found at http://eslint.org/docs/
{
  "env":{
    "browser":true,
    "node":true
  },
  "rules":{
    "curly":2,
    "eqeqeq":2,
    "quotes":[2,"single"],
    "indent":[2, 4],
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "no-alert":2,
    "no-eval":2,
    "no-native-reassign":2,
    "no-new":2,
    "no-octal":2,
    "no-proto":2,
    "no-underscore-dangle":0,
    "no-redeclare":2,
    "yoda":2,
    "key-spacing": [2, { "align": "value" }]
  }
}
