  },
  "homepage": "https://github.com/Coveo/analytics.js",
  "devDependencies": {
    "coveralls": "^2.11.2",
    "eslint": "^0.18.0",
    "jasmine-core": "^2.2.0",
    "karma": "^0.12.31",
    "karma-coverage": "^0.2.7",
    "karma-coveralls": "^0.1.5",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "uglify": "^0.1.3"
